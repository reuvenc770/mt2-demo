SELECT 3c.id, c.campaign_id, c.profile_id, substr(profile_name,1,8) AS profile_name, advertiser_name, brand_name, company, url, date_format(date(3c.scheduled_datetime), "%m-%d-%Y") AS sch_date FROM user u, campaign c, list_profile lp, 3rdparty_campaign 3c, client_brand_info cbi, advertiser_info ai,  brand_url_info bui WHERE c.campaign_id=3c.campaign_id AND 3c.advertiser_id=ai.advertiser_id AND 3c.brand_id=cbi.brand_id AND 3c.brand_id=bui.brand_id AND c.profile_id=lp.profile_id AND lp.client_id=3c.client_id AND u.user_id=3c.client_id AND 3c.id=$args->{id} AND bui.brand_id=293 AND 3c.client_id=9 AND url_type='O';
