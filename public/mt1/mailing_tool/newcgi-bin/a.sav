select advertiser_name,advertiser_from,sum(open_cnt)/sum(sent_cnt) from advertiser_from,email_log,advertiser_info where advertiser_info.advertiser_id=advertiser_from.advertiser_id and advertiser_from.from_id=email_log.from_id and campaign_id in (select campaign_id from 3rdparty_campaign where third_party_id in (5,6)) and advertiser_from.status='A' group by advertiser_name,advertiser_from order by 1,3 desc; 
